{
    "name": "dandelionmood/debrief",
    "description": "Debrief is an easy to use note-taking application crossed with a diary.",
    "keywords": ["debrief", "note-taking", "diary", "wiki"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "andywer/js-localization": "dev-laravel-5",
        "baum/baum": "^1.1",
        "davejamesmiller/laravel-breadcrumbs": "^5.1",
        "erusev/parsedown": "^1.7",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "5.7.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.7.0",
        "watson/validating": "^3.1"
    },
    "require-dev": {
        "filp/whoops": "~2.1.4",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "doctrine/dbal": "^2.6",
        "kkomelin/laravel-translatable-string-exporter": "^1.2",
        "caouecs/laravel-lang": "~3.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
